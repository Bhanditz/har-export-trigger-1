{
  "name": "HARExportTrigger",
  "short_name": "HARExportTrigger",
  "version": "0.0.1",
  "devtools_page": "src/devtools.html",
  "description": "Enable HAR export from within a web page",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content.js"
      ]
    }
  ]
}
